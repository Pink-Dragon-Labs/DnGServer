object SwampImp of FlyingImp
	properties
		special
		name "Swamp Imp"
		alignment 127

		minLevel 1600
		maxLevel 2000
		minDamage 2200
		maxDamage 4500

		strength 250
		endurance 35
		intelligence 175
		dexterity 20

		soundGroup 7
		armorType 1
		damageType 1
		range 2
		quickness 8
		  
		girth 80
		height 80

		# affects
		# do fire damage
		affect 1 0 0 -1

		# acid damage
		affect 4 0 0 -1

		# poison damage
		affect 5 0 0 -1

		# fire immolation
		affect 23 0 0 -1

		# cold immolation
		affect 24 0 0 -1

		# lightning immolation
		affect 27 0 0 -1
	end

	base container
		object RandomNewGear
		object RandomRareGlowDye
		object RandomRareHelmet
		object RandomMegaMoney
		object RandomMonsterMegaTreasure
	end

	base character
		skill 0 5
		skill 1 5
		skill 2 5
		skill 3 5
		skill 4 5
		skill 5 5
		skill 6 5
		skill 7 5
		skill 8 5
		skill 10 5
		## grand master wiz
		skill 11 5
		skill 12 5
		skill 13 5
		 
		skill 15 5
		skill 28 5
		skill 29 5
		properName "Swamp Imp"
	end

	actions
		vBeBorn changeSpellResistance self 75 2	#75% Resist Myst
		#  350% SDM to all
        vBeBorn changeSDM self 350 0 2 1 3 4
	end

	base npc
		code imp
	end
	
	base described
		text "A nasty Imp long since evolved from the typical Imp.\n\n 75% Myst Resistance.\n"
	end
end

