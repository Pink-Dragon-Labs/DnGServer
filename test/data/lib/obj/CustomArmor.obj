object EvilHighBoots of HighBoots
	properties
	special
		name "Evil High Boots"
		value 7500
		armor 50
		health 400
		color 97
		level 300
		special
	end

	base carryable
		weight 2.5
	end

	actions
		# Pos Dex (as per Boots of Nimbility)
		vBePutOn setAffect dobj 59 0 0 -1
		vBeTakenOff clearAffect dobj 59 0 0
