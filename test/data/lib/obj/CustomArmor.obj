object EvilThiefMask of ThiefMask
	properties
	special
		# Bandanna Special Version
		view 30950
		name "Evil Thieves' Mask"
		clutStart 63
		clutStart 21
		armorFlat 15
		level 425
		color 97
		enchantResistance 20
	end

	base wearable
				areaWorn 0
                # only Thieves
                mask warrior
                mask adventurer
                mask wizard
	end

	base carryable
		weight 1
	end

	actions
		vBePutOn clearAffect dobj 21 0 0
		# Magic Resist 15% to all except Myst
		vBePutOn changeSpellResistance dobj 15 0 1 3 4
		vBeTakenOff changeSpellResistance dobj -15 0 1 3 4
		# Free Will
		vBePutOn setAffect dobj 86 0 0 -1
		vBeTakenOff clearAffect dobj 86 0 0
	end

	base described
		text "You see a mask and bandanna."
		idText "You see a special mask and bandanna prized by thieves for its invisibility bestowing powers. You must be a level 425 thief, or higher, to wear this bandanna.\nEnchant Resistance: 20 percent.\n\n"
		shopText "These things are frowned upon by the local constabulary. Quick, put it away before someone sees you with it."
	end
end

